{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "verbatimModuleSyntax": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./src",
    "paths": {
      "@components/*": [
        "./components/*"
      ],
      "@layouts/*": [
        "./layouts/*"
      ],
      "@utils/*": [
        "./utils/*"
      ]
    }
  }
}